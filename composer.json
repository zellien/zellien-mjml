{
  "name": "zellien/zellien-mjml",
  "description": "PHP library for rendering MJML templates",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/zellien/zellien-mjml",
  "authors": [
    {
      "name": "Zellien",
      "email": "development.sites@gmail.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.1.99"
    }
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "ext-curl": "*",
    "psr/container": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Zellien\\Mjml\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zellien\\MjmlTest\\": "test/"
    }
  },
  "require-dev": {
    "vimeo/psalm": "^5.12",
    "phpunit/phpunit": "^10.2"
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
